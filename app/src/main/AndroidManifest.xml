<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.autoxss.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CALL_LOG"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask" />
        <activity android:name=".TestActivity" />
        <activity android:name=".StorageActivity" />
        <activity android:name=".HttpActivity"></activity>
        <activity android:name=".DataBaseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResolverActivity">

        </activity>

        <provider
            android:name=".provider.MyContentProvider"
            android:authorities="com.autoxss.myapplication.mycontentprovder"
            android:exported="true"></provider>
    </application>

</manifest>